@{
    ViewData["Title"] = "ResolutionCenter";
}
<div class="text-center">
    <h1 class="display-4">Resolution Center</h1>
</div>
<div class="container-fluid" style="margin-top:20px;">
    <h2>Open Returns</h2>
    <div>
        <table id="returnTable" class="table table-responsive table-bordered text-center">
            <thread>
                <tr>
                    <th hidden class="text-center">Id</th>
                    <th scope="col">Name</th>
                    <th scope="col">Qty</th>
                    <th scope="col">Total Price</th>
                    <th scope="col">Payment Method</th>
                    <th scope="col">Vendor</th>
                    <th scope="col">Date Sent Back</th>
                    <th scope="col">Delivery Date</th>
                    <th scope="col">Tracking #</th>
                    <th scope="col">Estimated Refund Time</th>
                </tr>
            </thread>
            <tbody>
                <tr>
                    <td hidden><input type="text" name="idreturns" id="idreturns" hidden /></td>
                    <td><input type="text" name="returnName" id="returnName" /></td>
                    <td><input type="text" name="quantity" id="quantity" /></td>
                    <td><input type="text" name="totalPrice" id="totalPrice" /></td>
                    <td>
                        <select id="paymentMethod" name="paymentMethod">
                            <option>Wells Fargo</option>
                            <option>Chase</option>
                            <option>American Express</option>
                            <option>Paypal</option>
                            <option>Adorama Edge</option>
                        </select>
                    </td>
                    <td><input type="text" name="returnVendor" id="returnVendor" /></td>
                    <td><input type="date" name="returnDate" id="returnDate" /></td>
                    <td><input type="date" name="deliveryDate" id="deliveryDate" /></td>
                    <td><input type="text" name="trackingNum" id="trackingNum" /></td>
                    <td><input type="text" name="estimatedRefundTime" id="estimatedRefundTime" /></td>
                </tr>
            </tbody>
        </table>
        <div class="text-right">
            <button type="button" id="addReturn" class="btn btn-success">Add Return</button>
        </div>
    </div>
    <h2>Open Insurance Claims</h2>
    <h2>Packages delayed in Transit </h2>
</div>
