CREATE TABLE `currentinventory` (
  `idCurrentInventory` int NOT NULL AUTO_INCREMENT,
  `Name` varchar(45) NOT NULL,
  `Qty` int DEFAULT NULL,
  `Price` float NOT NULL,
  `Vendor` varchar(45) NOT NULL,
  `PurchaseDate` datetime NOT NULL,
  `Warranty` varchar(45) DEFAULT NULL,
  `SerialNumbers` varchar(45) DEFAULT NULL,
  PRIMARY KEY (`idCurrentInventory`)
) ENGINE=InnoDB AUTO_INCREMENT=9 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;

CREATE TABLE `incominginventory` (
  `idincominginventory` int NOT NULL AUTO_INCREMENT,
  `Name` varchar(45) NOT NULL,
  `Qty` int DEFAULT NULL,
  `Price` float NOT NULL,
  `Vendor` varchar(45) NOT NULL,
  `PurchaseDate` datetime NOT NULL,
  `Warranty` varchar(45) NOT NULL,
  `SerialNumbers` varchar(45) NOT NULL,
  `TrackingNumber` varchar(45) DEFAULT NULL,
  PRIMARY KEY (`idincominginventory`)
) ENGINE=InnoDB AUTO_INCREMENT=20 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;

CREATE TABLE `topsellers` (
  `idTopSellers` int NOT NULL AUTO_INCREMENT,
  `tname` varchar(45) NOT NULL,
  `priceSold` float NOT NULL,
  `purchasePrice` float NOT NULL,
  `vendors` varchar(45) DEFAULT NULL,
  PRIMARY KEY (`idTopSellers`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;


